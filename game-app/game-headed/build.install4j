              <action name="Remove Windows Protocol Handler" id="176" beanClass="com.install4j.runtime.beans.actions.registry.DeleteRegistryItemAction" actionElevationType="elevated" errorMessage="Failed to unregister the protocol handler">
                <serializedBean>
                  <property name="keyName" type="string">Software\Classes\triplea</property>
                  <property name="onlyIfEmpty" type="boolean" value="false" />
                  <property name="registryRoot" type="enum" class="com.install4j.api.windows.RegistryRoot" value="HKEY_CURRENT_USER" />
                </serializedBean>
                <condition>Util.isWindows() &amp;&amp; ((String)WinRegistry.getValue(RegistryRoot.HKEY_CURRENT_USER, "Software\\Classes\\triplea\\shell\\open\\command", "")).equalsIgnoreCase("\"" + context.getVariable("sys.installationDir") + "\\TripleA.exe\" \"%1\"")</condition>
              </action>
              <action name="Remove Linux protocol handler" id="221" beanClass="com.install4j.runtime.beans.actions.control.RunScriptAction" errorMessage="Failed to remove protocol handler">
                <serializedBean>
                  <property name="script">
                    <object class="com.install4j.api.beans.ScriptProperty">
                      <property name="value" type="string">String home = System.getProperty("user.home");
File shareFolder = new File(home + "/.local/share");
if(System.getenv("XDG_DATA_HOME") != null || (shareFolder.exists() &amp;&amp; shareFolder.isDirectory())){
    File applicationsDir = new File(shareFolder, "applications");
    if(!applicationsDir.exists() || !applicationsDir.isDirectory()){
        return true;
    }
    File mimeappsFile = new File(applicationsDir, "mimeapps.list");
    if(!mimeappsFile.exists()){
        return true;
    }
    String mimeType = "x-scheme-handler/triplea";
    StringBuilder builder = new StringBuilder();
    try(Scanner scanner = new Scanner(mimeappsFile)){
        boolean isSetup = false;
        String mimeTypePrefix = mimeType + "=";
        while(scanner.hasNextLine()){
            String nextLine = scanner.nextLine();
            if(!nextLine.equalsIgnoreCase(mimeTypePrefix + "TripleA-" + context.getCompilerVariable("sys.version") + "-0.desktop")){
                builder.append(nextLine).append('\n');
            }
        }
    }
    try(PrintWriter writer = new PrintWriter(mimeappsFile)){
        writer.print(builder.toString());
    }
}
return true;</property>
                    </object>
                  </property>
                </serializedBean>
                <condition>Util.isLinux()</condition>
              </action>
            </actions>
